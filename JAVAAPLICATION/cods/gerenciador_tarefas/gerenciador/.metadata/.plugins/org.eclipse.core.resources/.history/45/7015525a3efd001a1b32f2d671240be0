package gerenciador.bento;

public class Gerenciador {

	public static void main(String[] args) {
		final Mes ABRIL = Mes.ABRIL;
		final Mes MAIO = Mes.MAIO;
		final Mes OUTUBRO = Mes.OUTUBRO;
		final Mes FEV = Mes.FEVEREIRO;
		
		Tarefa tarefa1 = new Tarefa("teste");
		Tarefa tarefa2 = new Tarefa("OutraTarefa", 31,ABRIL , 2020);
		Tarefa tarefa3 = new Tarefa("TarefaCompleta",13, MAIO, 2019, 14, 55, 40);
		Tarefa tarefa4 = new Tarefa("Tarefa4");
		Tarefa tarefa5 = new Tarefa("Tarefa5", 9, OUTUBRO, 1989);
		Tarefa tarefa6 = new Tarefa("Bissexto",30, FEV, 2012, 23, 59, 59);
		Tarefa tarefa7 = new Tarefa("NAO Bissexto", 29, FEV, 2011, 23, 59, 59);
		
		
		System.out.println(tarefa1);
		System.out.println();
		System.out.println(tarefa2);
		System.out.println();
		System.out.println(tarefa3);
		System.out.println();
		System.out.println(tarefa4);
		System.out.println();
		System.out.println(tarefa5);
		System.out.println();
		System.out.println(tarefa6);
		System.out.println();
		System.out.println(tarefa7);

	}

}
