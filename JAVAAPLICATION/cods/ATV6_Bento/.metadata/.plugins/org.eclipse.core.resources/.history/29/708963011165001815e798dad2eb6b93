package Sistema;



public final class Funcionario extends Pessoa {

	private final int cod;
	private TipoFuncionario cargo;
	
		
	public Funcionario(String nome, String CPF, int cod, TipoFuncionario funcao) {
		super(nome, CPF);		
		this.cod = cod;
		this.cargo = funcao;
	}
	
	

	public int getCod() {
		return cod;
	}

	@Override
	public String toString() {
		return "Funcion√°rio=" + cargo + ",Nome: " + getNome() + ", CPF="
				+ getCPF() + "Cod. = " + getCod();
	}
}
