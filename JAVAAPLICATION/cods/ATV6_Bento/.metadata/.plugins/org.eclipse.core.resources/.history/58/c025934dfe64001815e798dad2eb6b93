package Sistema;

import java.util.ArrayList;
import java.util.HashSet;

public final class Funcionario extends Pessoa {

	private final int CRECI, cod;
	
	private final HashSet <TipoFuncionario>  cargo = new HashSet<>();
	
	public Funcionario(String nome, String CPF, int creci, int cod) {
		super(nome, CPF);
		this.CRECI = creci;
		this.cod = cod;
	}
	
	public void addCargo(TipoFuncionario tp) {
		cargo.add(tp);
	}
	
	public HashSet<TipoFuncionario> getCargo() {
		return cargo;
	}

}
